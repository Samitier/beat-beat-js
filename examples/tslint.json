{
	"defaultSeverity": "warning",
	"extends": [
	  "tslint:recommended"
	],
	"linterOptions": {
	  "exclude": [
		"node_modules",
		".vscode"
	  ]
	},
	"rules": {
	  "quotemark": [true, "double"],
	  "semicolon": [true, "never"],
	  "indent": [true, "tabs", 2],
	  "interface-name": false,
	  "ordered-imports": false,
	  "object-literal-sort-keys": false,
	  "no-consecutive-blank-lines": false,
	  "trailing-comma": false,
	  "arrow-parens": false,
	  "member-access": false,
	  "curly": false,
	  "variable-name": false
	}
}
